# R2 Media Pipeline Configuration
# Copy this file to .env and fill in your values

# Cloudflare R2 Configuration
R2_ACCOUNT_ID=your-cloudflare-account-id
R2_ACCESS_KEY_ID=your-r2-access-key-id
R2_SECRET_ACCESS_KEY=your-r2-secret-access-key
R2_BUCKET=tdrealtyohio-media
R2_PUBLIC_BASE_URL=https://media.tdrealtyohio.com

# Optional: Openverse API Configuration
OPENVERSE_USER_AGENT=TDRealtyOhio/2.0 (https://tdrealtyohio.com)

# Legacy Media Pipeline (existing)
PEXELS_API_KEY=your-pexels-api-key
PIXABAY_API_KEY=your-pixabay-api-key
UNSPLASH_ACCESS_KEY=your-unsplash-access-key
